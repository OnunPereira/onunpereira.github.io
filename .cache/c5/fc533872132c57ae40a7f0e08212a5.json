 \"Arial\", sans-serif;\n  font-weight: 400;\n  font-size: 21px;\n  text-rendering: optimizeLegibility;\n  /* overflow-x: hidden; */\n}\n\n.grid-list {\n  padding: 16px;\n}\n\n.align-right {\n  align-self: flex-end;\n}\n\n.row {\n  display: flex;\n  flex-flow: row wrap;\n}\n\n.column {\n  display: flex;\n  flex-flow: column wrap;\n}\n\n.flex-end {\n  justify-content: flex-end;\n}\n\n.fill-height {\n  height: 100%;\n}\n\n/* section:last-of-type { margin-bottom: 60px; } */\n\n.section-title {\n  font-size: 200%;\n  margin: 24px 0;\n}\n\n.section-whoami {\n  /* padding-top: 120px; */\n  display: flex;\n  flex-flow: column;\n  justify-content: space-evenly;\n  margin: 16px 0;\n  padding: 16px;\n}\n\n/* .section-projects {\n  margin-top: 80px;\n} */\n\n/***** HEADER *****/\n\nheader {\n  color: #fff;\n  text-transform: upperCase;\n  background-image: linear-gradient(rgba(0, 0, 0, 0.7), rgba(0, 0, 0, 0.7)),\n    url(\"img/header.jpeg\");\n  background-size: cover;\n  background-position: center;\n  height: 100vh;\n}\n\n.title-box {\n  justify-content: center;\n  align-items: center;\n  flex: 1 0 auto;\n}\n\n.title-box h1 {\n  font-size: 400%;\n  font-weight: 100;\n  font-kerning: none;\n  letter-spacing: 0.5em;\n  border-bottom: 1px solid #e67e22;\n}\n\n.title-box h6 {\n  letter-spacing: 10px;\n}\n\n/***** NAVBAR *****/\n\n.main-nav {\n  list-style: none;\n}\n\n.main-nav li a:link,\n.main-nav li a:visited {\n  color: #fff;\n  text-decoration: none;\n  font-size: 90%;\n  border-bottom: 2px solid transparent;\n  transition: border 0.2s;\n  padding: 8px 0;\n}\n\n.main-nav li a:hover,\n.main-nav li a:active {\n  border-bottom: 2px solid #e67e22;\n}\n\n/***** STICKY NAVBAR *****/\n\n.sticky {\n  position: fixed;\n  top: 0;\n  left: 0;\n  width: 100%;\n  background-color: rgba(255, 255, 255, 0.98);\n  -webkit-box-shadow: 0 2px 2px #efefef;\n  box-shadow: 0 2px 2px #efefef;\n  z-index: 9999;\n}\n\n/* .sticky .main-nav { margin-top: 18px; } */\n\n.sticky .main-nav li a:link,\n.sticky .main-nav li a:visited {\n  color: #555;\n  padding: 18px 0;\n}\n\n.sticky .mobile-nav-icon {\n  margin: 10px auto;\n}\n\n.sticky .mobile-nav-icon i {\n  color: #555;\n}\n\n/* mobile nav */\n.mobile-nav-icon {\n  float: right;\n  margin: 20px 10px 0 0;\n  cursor: pointer;\n  display: none;\n}\n\n/***** ICONS *****/\n\n.icon-title {\n  float: left;\n  font-size: 125%;\n  margin: 12px 15px 0 0;\n  color: #e67e22;\n}\n\n/***** WHO AM I *****/\n\n.profile {\n  display: flex;\n  flex-direction: row;\n  justify-content: space-evenly;\n  align-items: center;\n  margin: 48px 0;\n}\n\n.profile img {\n  order: 2;\n  border-radius: 50%;\n  box-shadow: 0 6px 40px rgba(0, 0, 0, 0.3);\n}\n\n.profile p {\n  width: 60%;\n  line-height: 150%;\n}\n\n/***** PROJECTS *****/\n\n.section-projects {\n  background-color: #333;\n  margin: auto0;\n}\n\n.showcase {\n  display: flex;\n  list-style: none;\n  flex-flow: row wrap;\n}\n\n.showcase li {\n  width: 25%;\n}\n\n.project-photo {\n  position: relative;\n  overflow: hidden;\n}\n\n.project-photo img {\n  opacity: 0;\n  width: 100%;\n  transform: scale(1.1);\n  transition: opacity 0.5s, transform 0.5s;\n}\n\n.project-photo:hover img {\n  opacity: 0.9;\n  transform: scale(1.04);\n}\n\n.project-photo div {\n  transition: opacity 0.5s;\n}\n\n.project-photo h2 {\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  width: 90%;\n  color: #fff;\n  font-weight: 100;\n  text-align: center;\n  text-transform: uppercase;\n  transform: translate(-50%, -50%);\n}\n\n.project-photo:hover div {\n  opacity: 0;\n}\n\n/***** FOOTER *****/\n\nfooter {\n  background-color: #f0f0f0;\n  display: flex;\n  justify-content: center;\n}\n\n.media {\n  display: flex;\n  flex-flow: column wrap;\n  padding: 64px 0;\n}\n\n.media-links {\n  display: flex;\n  justify-content: space-evenly;\n  width: 100%;\n  padding-top: 16px;\n  list-style: none;\n}\n\n.media-link i {\n  font-size: 250%;\n}\n\n.ion-social-github,\n.ion-social-linkedin {\n  color: #555;\n  transition: color 0.2s;\n}\n\n.ion-social-linkedin:hover {\n  color: #0077b5;\n}\n\n.ion-social-github:hover {\n  color: #000;\n}\n"},"lineCount":null}},"error":null,"hash":"829ed17aff629bfa96ec13ef4759b162","cacheData":{"env":{}}}